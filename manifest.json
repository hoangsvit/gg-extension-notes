{
  "manifest_version": 3,
  "name": "ePlus Quick Notes",
  "version": "1.0",
  "description": "Create and manage quick notes in Google Chrome",
  "icons": {
    "16": "/img/notebook.png",
    "48": "/img/notebook.png",
    "128": "/img/notebook.png"
  },
  "permissions": ["storage"],
  "action": {
    "default_browser_action": "popup.html",
    "default_icon": "/img/notebook.png"
  }
}